plothanddf <- function(df, lab, commonmax=0, is.df=TRUE, cexletter=2.5) {
newmax <- ifelse(commonmax==0, max(df$C4), commonmax)
ylims <- c(0, newmax)
ylabel <- ifelse(is.df, "Detection probability", "Probability density function")
plot(df$C1, df$C2, type='l', lwd=3, lty=1, ylim=ylims, xlab="Distance", ylab=ylabel)
lines(c(0,0), c(df$C3[1], df$C4[1]))
lines(df$C3, df$C4)
text(0.9*max(df$C1), 0.95*newmax, cex=cexletter, font=2, labels=lab)
}
